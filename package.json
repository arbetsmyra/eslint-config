{
  "name": "@arbetsmyra/eslint-config",
  "version": "1.1.0",
  "description": "Arbetsmyra's ESLint config",
  "main": "./config/default.js",
  "scripts": {
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "eslint": "eslint .",
    "eslint:fix": "npm run eslint -- --fix",
    "test": "npm run eslint && npm run prettier:check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/arbetsmyra/eslint-config.git"
  },
  "keywords": [
    "eslint",
    "eslintconfig",
    "arbetsmyra"
  ],
  "author": "Anthon <anthon.kendel@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/arbetsmyra/eslint-config/issues"
  },
  "homepage": "https://github.com/arbetsmyra/eslint-config#readme",
  "peerDependencies": {
    "eslint": ">=4"
  },
  "devDependencies": {
    "@arbetsmyra/prettier-config": "1.2.1",
    "eslint": "7.29.0",
    "prettier": "2.3.1"
  },
  "prettier": "@arbetsmyra/prettier-config",
  "renovate": {
    "extends": [
      "@arbetsmyra"
    ]
  }
}
